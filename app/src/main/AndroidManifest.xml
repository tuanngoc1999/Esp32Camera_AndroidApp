<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.esp32app">

    <application
        android:name=".Notifi"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Esp32App">
        <activity android:name=".Common.MissionActivity"></activity>
        <activity android:name=".Admin.Member3"></activity>
        <activity android:name=".Admin.Member2" />
        <activity android:name=".Admin.Member1" />
        <activity android:name=".Admin.AboutUsActivity" />
        <activity android:name=".Common.LoginSignUp.StartScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

                <data android:scheme="com.package.name" />
            </intent-filter>
        </activity>
        <activity android:name=".User.ProfileActivity" />
        <activity android:name=".EnrollActivity" />
        <activity android:name=".Common.LoginSignUp.ForgotPwdActivity" />
        <activity android:name=".Common.LoginSignUp.RegisActivity" />
        <activity android:name=".Common.LoginSignUp.LoginActivity" />
        <activity android:name=".Common.MainActivity" />

        <service android:name=".Common.MyFirebaseService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>